{
  "name": "generoi/wp-paywall",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A paywall plugin",
  "homepage": "https://github.com/generoi/wp-paywall",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/generoi/wp-paywall/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=8.0.0"
  },
  "require-dev": {
    "laravel/pint": "^1.17"
  },
  "autoload": {
    "psr-4": {
      "GeneroWP\\Paywall\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/pint"
    ]
  },
  "archive": {
    "exclude": [
      ".gitignore"
    ]
  }
}
