{
  "name": "generoi/wp-paywall",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A paywall plugin",
  "homepage": "https://github.com/generoi/wp-paywall",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/generoi/wp-paywall/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "symfony/http-foundation": "^7.1",
    "firebase/php-jwt": "^6.10"
  },
  "require-dev": {
    "laravel/pint": "^1.17",
    "phpunit/phpunit": "^9.0",
    "wp-phpunit/wp-phpunit": "^6.6",
    "yoast/phpunit-polyfills": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "GeneroWP\\Paywall\\": "src/"
    }
  },
  "scripts": {
    "lint": [
      "vendor/bin/pint --test"
    ],
    "lint:fix": [
      "vendor/bin/pint"
    ]
  },
  "archive": {
    "exclude": [
      ".gitignore"
    ]
  }
}
